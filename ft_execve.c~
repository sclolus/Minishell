#include "minishell.h"

int32_t	ft_execve(char *filename, char *path, char **argv, char **env)
{
	static char oldpwd[MAX_PATH];

	if (getcwd(oldpwd, MAX_PATH) == -1)
		ft_putendl_fd("Error : getcwd() failed", 2);
	if (filename)
	{
		
	}
	if (chdir(oldpwd) == -1)
		ft_putendl_fd("chdir() failed", 2);
}
